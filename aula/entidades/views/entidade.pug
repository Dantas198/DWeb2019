extends layout
block content
    .w3-container
        .w3-card-4
            header.w3-container
                h1= entidade.sigla
                .w3-container
                    ul.w3-ul
                        li= entidade.sioe
                        li= entidade.internacional
                        li= entidade.designacao
                .w3-container
                    h2 Tipologias
                table.w3-table-all
                    tr  
                        th Id
                        th Sigla
                        th Designacao
                    each t in tipologias
                        tr
                            td=t.id
                            td=t.sigla
                            td=t.designacao 
                .w3-container
                    h2 Processos de que é dono
                table.w3-table-all
                    tr  
                        th Código
                        th Título
                    each p in dono
                        tr
                            td=p.codigo
                            td=p.titulo 
                .w3-container
                    h2 Processos em que intervem como participante
                table.w3-table-all
                    tr  
                        th Código
                        th Título
                    each p in participante
                        tr
                            td=p.codigo
                            td=p.titulo 
                    
            



                